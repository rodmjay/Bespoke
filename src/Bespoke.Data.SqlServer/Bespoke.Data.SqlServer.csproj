<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>microservice</PackageTags>
    <RepositoryUrl>https://github.com/rodmjay/bespoke</RepositoryUrl>
    <Description>Bespoke library for building microservices</Description>
    <Authors>Rod Johnson</Authors>
    <Version>1.0.0</Version>
    <PackageId>Bespoke.Data.SqlServer</PackageId>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>disable</Nullable>
		<RootNamespace>Bespoke.Data.SqlServer</RootNamespace>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="8.0.*" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Bespoke.Data\Bespoke.Data.csproj" />
	</ItemGroup>

</Project>
