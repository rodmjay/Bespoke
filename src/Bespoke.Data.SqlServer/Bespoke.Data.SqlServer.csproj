<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <NoWarn>NU1902</NoWarn>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>microservice</PackageTags>
    <RepositoryUrl>https://github.com/rodmjay/bespoke</RepositoryUrl>
    <Description>Bespoke library for building microservices</Description>
    <Authors>Rod Johnson</Authors>
    <Version>1.0.3</Version>
    <PackageId>Bespoke.Data.SqlServer</PackageId>
    <PackageReadmeFile>README.md</PackageReadmeFile>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>disable</Nullable>
		<RootNamespace>Bespoke.Data.SqlServer</RootNamespace>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Bespoke.Data\Bespoke.Data.csproj" />
	</ItemGroup>

	<ItemGroup>
		<!-- Include the README.md file in the package -->
		<None Include="README.md" Pack="true" PackagePath="" />
	</ItemGroup>

</Project>



