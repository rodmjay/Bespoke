<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
	  <NoWarn>NU1902</NoWarn>
	  <PackageLicenseExpression>MIT</PackageLicenseExpression>
	  <PackageTags>microservice</PackageTags>
	  <RepositoryUrl>https://github.com/rodmjay/bespoke</RepositoryUrl>
	  <Description>Bespoke library for building microservices</Description>
	  <Authors>Rod Johnson</Authors>
	  <Version>1.0.8</Version>
	  <PackageId>Bespoke.Data.PostgreSQL</PackageId>
	  <TargetFramework>net8.0</TargetFramework>
	  <ImplicitUsings>enable</ImplicitUsings>
	  <Nullable>disable</Nullable>
	  <RootNamespace>Bespoke.Data.PostgreSQL</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Bespoke.Data\Bespoke.Data.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Azure.Identity" />
    <PackageReference Include="Npgsql.EntityFrameworkCore.PostgreSQL" />
  </ItemGroup>

</Project>


